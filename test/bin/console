#!/usr/bin/env node

require('module-alias/register');
const fs = require('fs');
const Application = require('../vendor/console/Application');
const objectHelper = require('../vendor/helpers/object');

if(fs.existsSync('config/main-local.js')){
	config = objectHelper.merge(require('@config/main'), require('@config/main-local'));
}else{
	config = require('@config/main');
}

new Application(config).run();